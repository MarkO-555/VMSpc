<my:VPanelDlg 
        WindowStartupLocation="CenterOwner"          
        ResizeMode="NoResize"
        xmlns:my="clr-namespace:VMSpc.DlgWindows"
        xmlns:vs="clr-namespace:VMSpc.CustomComponents" 
        x:Class="VMSpc.UI.DlgWindows.ImagePanelDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VMSpc.UI.DlgWindows"
        mc:Ignorable="d"
        Title="ImagePanelDlg" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Name="ImagePath"></TextBox>
            <Button Name="CancelButton" Click="CancelButton_Click">Cancel</Button>
            <Button Name="ChangeImageButton" Click="ChangeImageButton_Click">Change</Button>
            <Button Name="OkButton" Click="OkButton_Click">Ok</Button>
        </StackPanel>
    </Grid>
</my:VPanelDlg>
