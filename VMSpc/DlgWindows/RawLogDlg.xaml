<my:VMSDialog 
        xmlns:my="clr-namespace:VMSpc.DlgWindows"
        x:Class="VMSpc.DlgWindows.RawLogDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VMSpc.DlgWindows"
        mc:Ignorable="d"
        Title="Raw Log" Height="200" Width="500" Background="LightGray">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" Width="400" Height="25" VerticalAlignment="Bottom" Margin="0,0,0,10" IsEnabled="False" Name="CurrentLogFile" />
        <Button Grid.Row="1" Width="60" Height="25" VerticalAlignment="Top" Name="ChangeLogFile">Change</Button>
    </Grid>
</my:VMSDialog>
